{
  "name": "lux-framework",
  "version": "0.0.1-beta.10",
  "description": "A MVC style Node.js framework for building lightning fast JSON APIs",
  "repository": "https://github.com/postlight/lux",
  "keywords": [
    "mvc",
    "api",
    "lux",
    "jsonapi",
    "json-api",
    "rest",
    "framework"
  ],
  "main": "dist/index.js",
  "bin": {
    "lux": "bin/lux"
  },
  "scripts": {
    "build": "npm run clean && npm run type-check && webpack",
    "clean": "rm -rf dist",
    "docs": "rm -rf docs && documentation build src -o docs -f html",
    "lint": "eslint src/**/*.js",
    "start": "lux serve",
    "test": "npm run build && mocha --timeout 60000 --compilers=js:babel-core/register test/helper.js test/unit/**/*.js test/integration/**/*.js",
    "type-check": "flow check"
  },
  "author": "Zachary Golba",
  "license": "MIT",
  "engines": {
    "node": ">= 4.0"
  },
  "dependencies": {
    "ansi-regex": "2.0.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "bluebird": "3.4.0",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "eslint": "2.11.0",
    "eslint-loader": "1.3.0",
    "inflection": "1.10.0",
    "json-loader": "0.5.4",
    "memory-fs": "0.3.0",
    "moment": "2.13.0",
    "ora": "0.2.3",
    "require-reload": "0.2.2",
    "source-map-support": "0.4.0",
    "webpack": "2.1.0-beta.7"
  },
  "devDependencies": {
    "babel-core": "6.9.0",
    "babel-plugin-syntax-flow": "6.8.0",
    "babel-plugin-syntax-trailing-function-commas": "6.8.0",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-class-properties": "6.9.0",
    "babel-plugin-transform-decorators": "6.8.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-destructuring": "6.9.0",
    "babel-plugin-transform-es2015-parameters": "6.9.0",
    "babel-plugin-transform-es2015-spread": "6.8.0",
    "babel-plugin-transform-exponentiation-operator": "6.8.0",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "chai": "3.5.0",
    "documentation": "4.0.0-beta3",
    "flow-bin": "0.25.0",
    "isomorphic-fetch": "2.2.1",
    "mocha": "2.5.3"
  }
}
